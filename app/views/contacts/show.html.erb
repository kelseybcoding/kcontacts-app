<h1>Contact</h1>
<h6>Last Updated On: <%= @contact.friendly_updated_at %></h6>

<h3>First Name: <%= @contact.first_name %></h3>
<h3>Middle Name: <%= @contact.middle_name %></h3>
<h3>Last Name: <%= @contact.last_name %></h3>
<h3>Full Name: <%= @contact.full_name %> </h3>
<h3>Phone Number: <%= @contact.phone_number %></h3>
<h3>Email: <%= @contact.email %></h3>
<h3>Bio: <%= @contact.bio %></h3>

<%= link_to "Edit", "/contacts/#{@contact.id}/edit", class: "btn btn-success" %>
<%= link_to "Delete", "/contacts/#{@contact.id}", {method: :delete, class: "btn btn-danger" }%>
  